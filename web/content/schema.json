{
  "$defs": {
    "ColumnMetadata": {
      "description": "Model representing metadata for a single column in the BRFSS dataset.\nContains information parsed from the codebook including variable details,\nassociated question text, and possible values.",
      "properties": {
        "computed": {
          "title": "Computed",
          "type": "boolean"
        },
        "label": {
          "title": "Label",
          "type": "string"
        },
        "sas_variable_name": {
          "title": "Sas Variable Name",
          "type": "string"
        },
        "section_name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Section Name"
        },
        "section_number": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Section Number"
        },
        "module_number": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Module Number"
        },
        "question_number": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Question Number"
        },
        "column": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Column"
        },
        "type_of_variable": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Type Of Variable"
        },
        "question_prologue": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Question Prologue"
        },
        "question": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Question"
        },
        "value_lookup": {
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/ValueDef"
              },
              {
                "$ref": "#/$defs/ValueRange"
              }
            ]
          },
          "title": "Value Lookup",
          "type": "array"
        },
        "html_name": {
          "title": "Html Name",
          "type": "string"
        }
      },
      "required": [
        "computed",
        "label",
        "sas_variable_name",
        "value_lookup",
        "html_name"
      ],
      "title": "ColumnMetadata",
      "type": "object"
    },
    "ValueDef": {
      "description": "Base model for representing value definitions in BRFSS survey data.",
      "properties": {
        "description": {
          "title": "Description",
          "type": "string"
        }
      },
      "required": [
        "description"
      ],
      "title": "ValueDef",
      "type": "object"
    },
    "ValueRange": {
      "description": "Model for value definitions that have a numeric range (single value or range of values).",
      "properties": {
        "description": {
          "title": "Description",
          "type": "string"
        },
        "start": {
          "title": "Start",
          "type": "integer"
        },
        "end": {
          "title": "End",
          "type": "integer"
        }
      },
      "required": [
        "description",
        "start",
        "end"
      ],
      "title": "ValueRange",
      "type": "object"
    }
  },
  "description": "Container model for all column metadata to be exported as JSON.",
  "properties": {
    "columns": {
      "additionalProperties": {
        "$ref": "#/$defs/ColumnMetadata"
      },
      "title": "Columns",
      "type": "object"
    }
  },
  "required": [
    "columns"
  ],
  "title": "SharedModel",
  "type": "object"
}